     "output_type": "display_data"
    }
   ],
   "source": [
    "z=np.arange(-4,4,0.01)\n",
    "plt.plot(z,gelu(z),label='GeLU')\n",
    "plt.legend()\n",
    "plt.grid()\n",
    "plt.show()"
   ]
  }
